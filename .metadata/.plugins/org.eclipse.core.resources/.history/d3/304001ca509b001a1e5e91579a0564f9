    <script>
    	$(function(){
			setInterval(function(){
				if($(window	).scrollTop()>=110){
					$("#main_nav").animate({top:0},500);
				}else{
					$("#main_nav").animate({top:-80},500);
				}
			},1000);
		});
    </script>
